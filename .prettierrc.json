{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "jsxSingleQuote": false,
  "htmlWhitespaceSensitivity": "ignore",
  "insertPragma": false,

  "vueIndentScriptAndStyle": false,
  "singleAttributePerLine": false
}
